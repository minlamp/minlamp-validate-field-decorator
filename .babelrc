{
    "presets": [
        [
            "@babel/preset-env", 
            {
                "modules": false,
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 2 versions",
                        "not ie <= 8"
                    ]
                }
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/transform-runtime",
        "@babel/plugin-proposal-class-properties"
    ]
}